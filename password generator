import random

print("welcome to password generator!!!")
randomchars = "ABCDEFGHIJKLMNOPQRSTUVWXYZ012345678910!@#$%&"
noofpwds = int(input("please enter the number of passwords to be generate : "))
pwdslen = int(input("please enter the lenght of password you needed : "))

print("Here are your random passwords: ")
for x in range(noofpwds):
    pwd = ""
    for char in range(pwdslen):
        pwd = pwd + random.choice(randomchars)
    print(pwd)
